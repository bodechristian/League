<VBox id="vbox" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.CntrlOverview" spacing="15">
	<padding>
		<Insets bottom="50" left="50" right="50" top="25" />
	</padding>
	<HBox id="hbox" fx:id="hbTools" spacing="8" alignment="CENTER_LEFT">
		<Label id="label" alignment="CENTER" text="Filter by Role:" />
		<ComboBox id="cb" fx:id="cbRoles" onAction="#cbChanged" />
	</HBox>
	<StackPane id="fp" fx:id="overviewWindow" prefHeight="700"
		prefWidth="1080">
	</StackPane>
</VBox>

